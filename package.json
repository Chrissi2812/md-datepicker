{
  "name": "md-datepicker",
  "version": "1.1.1",
  "description": "Material Design inpired date- and timepicker",
  "scripts": {
    "build:css": "autoprefixer < src/css/md-datepicker.css | cssmin > dist/%npm_package_name: =_%.min.css",
    "build:js": "uglifyjs src/js/moment.js src/js/md-datepicker.js --screw-ie8 -m --source-map dist/%npm_package_name: =_%.min.map --source-map-url %npm_package_name: =_%.min.map > dist/%npm_package_name: =_%.min.js",
    "build:js-without": "uglifyjs src/js/md-datepicker.js --screw-ie8 -m --source-map maps/%npm_package_name: =_%.dep.min.map --source-map-url %npm_package_name: =_%.dep.min.map > dist/%npm_package_name: =_%.dep.min.js",
    "build": "del dist /F /Q & npm run build:js & npm run build:js-without & npm run build:css",
    "publish": "npm run build & npm run commit & git push origin master",
    "commit": "git add --a & git commit -m publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Chrissi2812/md-datepicker.git"
  },
  "keywords": [
    "date-picker",
    "materialdesign"
  ],
  "author": "Chrissi2812",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chrissi2812/md-datepicker/issues"
  },
  "homepage": "https://github.com/Chrissi2812/md-datepicker",
  "devDependencies": {
    "autoprefixer": "latest",
    "cssmin": "latest",
    "uglify-js": "latest"
  }
}
