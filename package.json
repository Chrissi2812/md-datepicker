{
  "name": "md-datepicker",
  "version": "1.0.1",
  "description": "Material Design inpired date- and timepicker",
  "main": "index.js",
  "scripts": {
    "build:css": "autoprefixer < css/md-datepicker.css | cssmin > dist/md-datepicker.min.css",
    "build:js": "uglifyjs js/moment.js js/md-datepicker.js > dist/md-datepicker.min.js",
    "build": "npm run build:js & npm run build:css"
  },
  "repository": {
