  },
  "keywords": [
    "date-picker",
    "materialdesign"
  ],
  "author": "Chrissi2812",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chrissi2812/md-datepicker/issues"
  },
  "homepage": "https://github.com/Chrissi2812/md-datepicker",
  "devDependencies": {
    "autoprefixer": "latest",
    "cssmin": "latest",
    "uglify-js": "latest"
  }
}
